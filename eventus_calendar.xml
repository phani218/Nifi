          <entry>
            <key>SQL select query</key>
            <value>SELECT 
B.name building_name,
A.location,
group_concat(c.name SEPARATOR '-') calendars,
A.name event_name,
A.id event_id,
A.description,
A.allDay allday_event,
A.startDate startdate,
A.startTime starttime,
A.status ,
A.endDate enddate,
A.endTime endtime,
A.contactName contactname,
A.contactEmail contactemail,
A.contactPhone contactphone
from 
eventus.events A
left join eventus.buildings B
on A.buildingId = B.id
left join 
eventus.event_calendars ec
on ec.eventid=A.id
left join eventus.calendars c
on ec.calendarId=c.id
where
year(a.startDate) ='2017' and
 accessLevel='public'
group by
A.id</value>
